{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/kysely-adapter": [
        "libs/kysely-adapter/src"
      ],
      "@app/kysely-adapter/*": [
        "libs/kysely-adapter/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/request": [
        "libs/request/src"
      ],
      "@app/request/*": [
        "libs/request/src/*"
      ],
      "@app/response": [
        "libs/response/src"
      ],
      "@app/response/*": [
        "libs/response/src/*"
      ],
      "@app/schema": [
        "libs/schema/src"
      ],
      "@app/schema/*": [
        "libs/schema/src/*"
      ],
      "@app/auth-server": [
        "libs/auth-server/src"
      ],
      "@app/auth-server/*": [
        "libs/auth-server/src/*"
      ]
    }
  }
}