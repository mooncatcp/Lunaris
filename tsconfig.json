{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/kysely-adapter": [
        "libs/kysely-adapter/src"
      ],
      "@app/kysely-adapter/*": [
        "libs/kysely-adapter/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/response": [
        "libs/response/src"
      ],
      "@app/response/*": [
        "libs/response/src/*"
      ],
      "@app/schema": [
        "libs/schema/src"
      ],
      "@app/schema/*": [
        "libs/schema/src/*"
      ],
      "@app/keystore-server": [
        "libs/keystore-server/src"
      ],
      "@app/keystore-server/*": [
        "libs/keystore-server/src/*"
      ],
      "@app/crypto": [
        "libs/crypto/src"
      ],
      "@app/crypto/*": [
        "libs/crypto/src/*"
      ],
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/members": [
        "libs/members/src"
      ],
      "@app/members/*": [
        "libs/members/src/*"
      ],
      "@app/channels": [
        "libs/channels/src"
      ],
      "@app/channels/*": [
        "libs/channels/src/*"
      ],
      "@app/permissions": [
        "libs/permissions/src"
      ],
      "@app/permissions/*": [
        "libs/permissions/src/*"
      ],
      "@app/snowflake": [
        "libs/snowflake/src"
      ],
      "@app/snowflake/*": [
        "libs/snowflake/src/*"
      ]
    }
  }
}