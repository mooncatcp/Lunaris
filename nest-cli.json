{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "dtoFileNameSuffix": [".dto.ts", ".schema.ts"],
          "controllerFileNameSuffix": ".controller.ts",
          "introspectComments": true
        }
      }
    ]
  },
  "projects": {
    "kysely-adapter": {
      "type": "library",
      "root": "libs/kysely-adapter",
      "entryFile": "index",
      "sourceRoot": "libs/kysely-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kysely-adapter/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "response": {
      "type": "library",
      "root": "libs/response",
      "entryFile": "index",
      "sourceRoot": "libs/response/src",
      "compilerOptions": {
        "tsConfigPath": "libs/response/tsconfig.lib.json"
      }
    },
    "schema": {
      "type": "library",
      "root": "libs/schema",
      "entryFile": "index",
      "sourceRoot": "libs/schema/src",
      "compilerOptions": {
        "tsConfigPath": "libs/schema/tsconfig.lib.json"
      }
    },
    "keystore-server": {
      "type": "library",
      "root": "libs/keystore-server",
      "entryFile": "index",
      "sourceRoot": "libs/keystore-server/src",
      "compilerOptions": {
        "tsConfigPath": "libs/keystore-server/tsconfig.lib.json"
      }
    },
    "crypto": {
      "type": "library",
      "root": "libs/crypto",
      "entryFile": "index",
      "sourceRoot": "libs/crypto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crypto/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "members": {
      "type": "library",
      "root": "libs/members",
      "entryFile": "index",
      "sourceRoot": "libs/members/src",
      "compilerOptions": {
        "tsConfigPath": "libs/members/tsconfig.lib.json"
      }
    },
    "permissions": {
      "type": "library",
      "root": "libs/permissions",
      "entryFile": "index",
      "sourceRoot": "libs/permissions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/permissions/tsconfig.lib.json"
      }
    },
    "snowflake": {
      "type": "library",
      "root": "libs/snowflake",
      "entryFile": "index",
      "sourceRoot": "libs/snowflake/src",
      "compilerOptions": {
        "tsConfigPath": "libs/snowflake/tsconfig.lib.json"
      }
    }
  }
}